{
	"name": "ytm-op",
	"version": "1.0.8",
	"description": "This is a Youtube Music Client with a Discord Rich Presense, janky VRChat OSC support and only if you know how to run it",
	"main": "src/main.js",
	"scripts": {
		"test": "electron .",
		"testTwo": "yarn lint",
		"lint": "eslint **/*.js",
		"lint:fix": "eslint --fix **/*.js",
		"start": "electron . --trace-warnings",
		"start-ci": "start-ci.bat",
		"pack": "electron-builder --dir",
		"package-linux": "electron-packager . YTMOP --platform=linux --arch=x64",
		"dist:mac": "electron-builder --mac",
		"dist:win": "electron-builder --win --x64 --ia32",
		"dist:win32": "electron-builder --win --ia32",
		"dist:win64": "electron-builder --win --x64",
		"dist:mw": "yarn dist:win && yarn dist:mac",
		"preinstall": "npx npm-force-resolutions",
		"dist:linux": "electron-builder --linux --x64 --ia32",
		"dist": "yarn dist:mw && yarn dist:linux"
	},
	"keywords": [
		"youtube",
		"music",
		"client",
		"discord",
		"rpc",
		"rich",
		"presence"
	],
	"author": "FennBoii",
	"license": "ISC",
	"build": {
		"productName": "YTM-OP",
		"appId": "ytm_op",
		"mac": {
			"category": "ytm_op",
			"icon": "resources/assets/build/osx/icon.icns",
			"target": [
				"zip",
				"dmg"
			]
		},
		"win": {
			"icon": "resources/assets/build/win/icon.ico"
		},
		"nsis": {
			"artifactName": "${productName}-Setup-${version}.${ext}",
			"perMachine": true
		},
		"linux": {
			"target": "AppImage",
			"icon": "resources/assets/build/linux/icon_2048x2048.png",
			"synopsis": "ytm_op",
			"description": "A small Electron app for YouTube Music which also integrates with Discord",
			"maintainer": "Fennec_",
			"category": "Music"
		},
		"directories": {
			"buildResources": "resources/assets/*",
			"output": "dist"
		},
		"files": [
			"**/*",
			"!dist/*",
			"!dist_old/*"
		],
		"extraResources": [
			"app.manifest",
			{
				"from": "config.json",
				"to": "extra/config.json"
			}
		],
		"extraFiles": [
			"resources/assets/images/*.png",
			{
				"from": "config.json",
				"to": "resources",
				"filter": [
					"**/*"
				]
			},
			{
				"from": "config.json",
				"to": "%home%",
				"filter": [
					"**/*"
				]
			},
			{
				"from": "VolumeFind.exe",
				"to": ".",
				"filter": [
					"**/*"
				]
			},
			{
				"from": "NAudio.Wasapi.dll",
				"to": ".",
				"filter": [
					"**/*"
				]
			}
		]
	},
	"dependencies": {
		"axios": "^1.6.7",
		"blessed": "^0.1.81",
		"builder-util-runtime": "^9.2.2",
		"cli-spinners": "^2.9.2",
		"cli-table3": "^0.6.3",
		"clipboardy": "^4.0.0",
		"discord-rpc": "^4.0.1",
		"dotenv": "^16.4.1",
		"easy-volume": "^1.1.0",
		"electron-context-menu": "^3.3.0",
		"electron-localshortcut": "^3.2.1",
		"html-to-text": "^9.0.5",
		"jpeg-js": "^0.4.4",
		"jsdom": "^24.0.0",
		"loudness": "^0.4.2",
		"minimist": "^1.2.8",
		"mkdirp": "^3.0.1",
		"node": "^21.1.0",
		"node-addon-api": "^7.1.0",
		"node-alsa-ctl": "^0.0.2",
		"node-fetch": "^3.3.2",
		"node-gyp": "^10.0.0",
		"npm-force-resolutions": "^0.0.10",
		"osc": "^2.4.4",
		"path": "^0.12.7",
		"prettier": "^3.0.3",
		"prompt": "^1.3.0",
